

<view class="{{isIPhoneX?'padX':''}}">
<view wx:for="{{manuallist}}" wx:for-item="manuallist">
  <view class="content" bindtap='xjjgxq' data-draft_quote_id='{{manuallist.draft_quote_id}}' data-selectprice_id='{{manuallist.selectprice_id}}'>
    <view class='content-top'>

       <view wx:if="{{manuallist.sign ==1}}">
        <image src='../../images/xjjg_03.png' class='img-top'></image>
      </view>

      
      <text class='x1'>{{manuallist.company_name}}</text>
      <text class='x2 fl'>机构身份：</text>

      <view wx:if="{{manuallist.authe_status ==1}}">
        <image src='../../images/xjjg_01.png' class='img fl'></image>
      </view>

      <view wx:if="{{manuallist.authe_status == 0}}">
        <image src='../../images/xjjg_02.png' class='img fl'></image>
      </view>    
      
      <view class='clear'></view>
    </view>
    <view class='content-bottom flex'>
      <view class='list'>
        <text>{{manuallist.place_name}}</text>
        <text>{{manuallist.province_name}}</text>
      </view>
      <view class='list'>
        <text>{{manuallist.interest_name}}</text>
        <text>{{manuallist.data_type_info}}</text>
      </view>
      <view class='list'>
        <text>{{manuallist.money_name}}</text>
        <text>{{manuallist.deduct_money}}</text>
      </view>
    </view>
  </view>
</view>
</view>



