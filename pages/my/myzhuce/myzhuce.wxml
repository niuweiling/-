<image src="../../images/logo.png" class="logo-icon"></image>
<image src='../../images/logo-text.png' class='logo-text'></image>
<view class='list a1'>
  <input placeholder='手机号' bindinput="blurphone" value="{{phone}}" type='number' maxlength='11'></input>
</view>
<view class='list a2 flex'>

  
  <!-- <input placeholder='密码'></input>
  <image src='../../images/logo_01.png' class='password hide'></image>
  <image src='../../images/logo_02.png' class='password'></image> -->


  <input placeholder='{{pswvalue}}' style="display:{{a2psw}}" maxlength='25' password value="{{password}}"  bindinput="blurpsw" ></input>

  <input placeholder='{{pswvalue}}' bind style="display:{{a2psw2}}" maxlength='25' value="{{password}}"  bindinput="blurpsw" ></input>

 
  <image src='../../images/logo_01.png' class="password hide{{showView?'show':''}}"  bindtap='eyeclick'></image>
  <image src='../../images/logo_02.png' class="password hide{{showView?'':'show'}}" bindtap='eyeclick'></image>




</view>
<view class='yzm flex'>
  <input placeholder='验证码' bindinput="blurverify"></input>
  <view class='jg'></view>

  <text class="{{is_show?'show':'hide'}} hq" bindtap="clickVerify">获取验证码</text>
  <text class="{{is_show?'hide':'show'}} hq">重新发送{{last_time}}秒</text>


  <!-- <text class='hq'  bindtap='verifyreg'>获取验证码</text> -->
</view>
<view class='xy flex'>
  <block  wx:if="{{from == 'zhuce'}}">
  <checkbox checked='{{checkboxChange}}' bindtap="checkboxChange"></checkbox>
  <view class='agree' bindtap='openxy'>同意《云票据服务协议》</view>
  </block>
</view>
<button class='dl' bindtap='register'>提交</button>
